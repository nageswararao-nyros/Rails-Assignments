<div class="jumbotron">
	<div align="center">
		<div class="form-align">
			<div align="left" class="formHeader-align">
				<h3  class="btn btn-info btn-sm"><b>New Publisher</b></h3>
			</div>
		</div>
			<div class="form-align">
				<div class="formContent-align">

					<%= form_with model:@publisher do |form| %>

					<div class="form-group">
						<div align="left"><%= form.label :name %></div>
						<%= form.text_field :name , {:class=>"form-control"} %>
					</div>

					<div class="form-group"> 
						<div align="left"><%= form.label :from %></div>
						<%= form.text_field :from , {:class=>"form-control"}%>
					</div>
					

					<div class="row">
						<div align="left" class="col-6">
							<%= link_to 'Back', articles_path, {:class=>"btn btn-warning"} %>
						</div>
						<div align="right" class="col-6">
							<%= form.submit :save,{:class=>"btn btn-success"} %>
						</div>
					</div>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>